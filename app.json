{
  "expo": {
    "name": "Christoffel's Kitchen",
    "slug": "christoffels-kitchen",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#C87941"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.christoffels.kitchen"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#C87941"
      },
      "package": "com.christoffels.kitchen"
    },
    "plugins": [
      "expo-font"
    ],
    "scheme": "christoffelskitchen"
  }
}